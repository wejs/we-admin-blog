{
  "name": "we-admin-blog",
  "version": "0.0.0",
  "description": "We.js plugin with ember.js for add admin blog SPA in /admin route",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test-server": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage-server": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js",
    "inspect": "node --inspect --debug-brk app.js",
    "watch": "supervisor -w lib,server,config app.js",
    "deploy": "git push prod master:prod"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "files": [
    "dist",
    "install.js",
    "plugin.js",
    "README.md"
  ],
  "license": "MIT",
  "repository": "wejs/we-admin-blog",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-ajax": "^2.0.1",
    "ember-bootstrap": "1.0.0-alpha.3",
    "ember-cli": "2.6.2",
    "ember-cli-active-link-wrapper": "0.3.2",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-moment-shim": "3.0.1",
    "ember-cli-notifications": "4.1.6",
    "ember-cli-pace": "0.1.0",
    "ember-cli-qunit": "^1.4.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-tinymce": "0.1.5",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.6.0",
    "ember-export-application-global": "^1.0.5",
    "ember-font-awesome": "2.2.0",
    "ember-i18n": "5.0.0",
    "ember-load-initializers": "^0.5.1",
    "ember-metismenu": "0.0.3",
    "ember-models-table": "1.11.0",
    "ember-moment": "7.3.0",
    "ember-resolver": "^2.0.3",
    "ember-simple-auth": "1.2.0-beta.2",
    "ember-sortable": "1.9.1",
    "loader.js": "^4.0.1",
    "we-plugin-user-settings": "^1.0.0",
    "fs-extra": "^2.0.0",
    "we-plugin-i18n-api": "^1.0.0"
  },
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "dependencies": {
      "we-plugin-user-settings": true,
      "we-passport-oauth2-password": true,
      "we-plugin-i18n-api": true
    }
  },
  "ember-addon": {
    "paths": [
      "lib/copy-build-files"
    ]
  }
}
